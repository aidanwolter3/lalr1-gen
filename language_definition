E -> 12
L -> 13
B -> 14

%%

S' -> S
S -> S S
S ->
S -> id assign E
S -> huck E
E -> num
E -> id
E -> E B E
L -> E comma L
L -> E
B -> plus
B -> minus
B -> mult
B -> div

